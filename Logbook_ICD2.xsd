<xs:schema xmlns:elb="http://www.ataebiz.org/XMLSchema/DSELogbook" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xades="http://uri.etsi.org/01903/v1.3.2#" xmlns:ds="http://www.w3.org/2000/09/xmldsig#" targetNamespace="http://www.ataebiz.org/XMLSchema/DSELogbook" elementFormDefault="qualified" attributeFormDefault="unqualified" version="4.01">
	<xs:import namespace="http://uri.etsi.org/01903/v1.3.2#" schemaLocation="http://uri.etsi.org/01903/v1.3.2/XAdES.xsd"/>
	<xs:import namespace="http://www.w3.org/2000/09/xmldsig#" schemaLocation="http://www.w3.org/TR/2002/REC-xmldsig-core-20020212/xmldsig-core-schema.xsd"/>
	<xs:annotation>
		<xs:documentation>====  Top Level Elements  ====</xs:documentation>
	</xs:annotation>
	<xs:element name="ATA_DSE_Logbook" type="elb:ATA_DSE_LogbookType">
		<xs:annotation>
			<xs:documentation>Top Level element defined for backward compatibility</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="LogbookData" type="elb:ATA_DSE_LogbookType">
		<xs:annotation>
			<xs:documentation>Top Level element for logbook object data only</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ATA_S2000_Ch17_Data" type="elb:ATA_S2000_Ch17_DataType">
		<xs:annotation>
			<xs:documentation>Top Level element which allows for multiple LogbookDataContainer elements</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ATA_DSE_LogbookResponse" type="elb:LogbookResponseType">
		<xs:annotation>
			<xs:documentation>Top Level element used for response messages</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:annotation>
		<xs:documentation>====  Top Level Out-of-band Service Elements  ====</xs:documentation>
	</xs:annotation>
	<xs:element name="PartValidationRequest" type="elb:PartValidationRequestType">
		<xs:annotation>
			<xs:documentation>Top Level element used by the client of a part validation interaction to request part validation support for a part related transaction</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="PartValidationResponse" type="elb:PartValidationResponseType">
		<xs:annotation>
			<xs:documentation>Top Level element used by the server of a part validation interaction in response to a PartValidationRequest from a client</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:annotation>
		<xs:documentation>====  Top Level Complex Types  ====</xs:documentation>
	</xs:annotation>
	<xs:complexType name="ATA_DSE_LogbookType">
		<xs:annotation>
			<xs:documentation>Root ATA DSE Logbook Element Definition</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Asset" type="elb:AssetType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The asset (aircraft or component) which is referred to by other logbook data</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="ELB_User" type="elb:ELB_UserType">
					<xs:annotation>
						<xs:documentation>Represents information about the person who entered or signed the logbook data</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ELB_Signature" type="elb:ELB_SignatureType">
					<xs:annotation>
						<xs:documentation>Represents logbook signature information (other than digital signatures)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="FlightLog" type="elb:FlightLogType">
					<xs:annotation>
						<xs:documentation>Represents both general flight and specific journey log information</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="FuelRecord" type="elb:FuelRecordType">
					<xs:annotation>
						<xs:documentation>Represents fuel record / fuel logbook information</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="MaintAction" type="elb:MaintActionType">
					<xs:annotation>
						<xs:documentation>Represents records documenting the performance of maintenance activity</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="MaintActionPartReplacement" type="elb:MaintActionPartReplacementType">
					<xs:annotation>
						<xs:documentation>Represents part removal and installation activity</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="MaintActionTask" type="elb:MaintActionTaskType">
					<xs:annotation>
						<xs:documentation>Represents maintenance tasks performed as part of an overall maintenance action</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="MaintLog" type="elb:MaintLogType">
					<xs:annotation>
						<xs:documentation>Captures the types of information which drive line maintenance activity: Fault Reports, Work Orders, Requests for Action</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="MaintLogResource" type="elb:MaintLogResourceType">
					<xs:annotation>
						<xs:documentation>Resources (parts, personnel, skills, hangar space, etc.) required to resolve a defect and/or those resources used as part of a maintenance action</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="MaintRelease" type="elb:MaintReleaseType">
					<xs:annotation>
						<xs:documentation>Documents airworthiness release</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ReferenceData" type="elb:ReferenceDataType">
					<xs:annotation>
						<xs:documentation>Represents attachment type data referenced in other records</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ServiceLog" type="elb:ServiceLogType">
					<xs:annotation>
						<xs:documentation>Represents aircraft servicing actions taken</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="JoinFRFL">
					<xs:annotation>
						<xs:documentation>Relationship information joining FuelRecord and FlightLog records</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="elb:JoinBaseType">
								<xs:sequence>
									<xs:element name="FuelRecordID" type="elb:EntityID_Type"/>
									<xs:element name="FlightLogID" type="elb:EntityID_Type"/>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="JoinMAFL">
					<xs:annotation>
						<xs:documentation>Relationship information joining MaintAction and FlightLog records</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="elb:JoinBaseType">
								<xs:sequence>
									<xs:element name="MaintActionID" type="elb:EntityID_Type"/>
									<xs:element name="FlightLogID" type="elb:EntityID_Type"/>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="JoinMAML">
					<xs:annotation>
						<xs:documentation>Relationship information joining MaintAction and MaintLog records</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="elb:JoinBaseType">
								<xs:sequence>
									<xs:element name="MaintActionID" type="elb:EntityID_Type"/>
									<xs:element name="MaintLogID" type="elb:EntityID_Type"/>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="JoinMAMP">
					<xs:annotation>
						<xs:documentation>Relationship information joining MaintAction and MaintActionPartReplacement records</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="elb:JoinBaseType">
								<xs:sequence>
									<xs:element name="MaintActionID" type="elb:EntityID_Type"/>
									<xs:element name="MaintActionPartReplacementID" type="elb:EntityID_Type"/>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="JoinMAMT">
					<xs:annotation>
						<xs:documentation>Relationship information joining MaintAction and MaintActionTask records</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="elb:JoinBaseType">
								<xs:sequence>
									<xs:element name="MaintActionID" type="elb:EntityID_Type"/>
									<xs:element name="MaintActionTaskID" type="elb:EntityID_Type"/>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="JoinMARS">
					<xs:annotation>
						<xs:documentation>Relationship information joining MaintAction and MaintLogResource records</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="elb:JoinBaseType">
								<xs:sequence>
									<xs:element name="MaintActionID" type="elb:EntityID_Type"/>
									<xs:element name="MaintLogResourceID" type="elb:EntityID_Type"/>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="JoinMLFL">
					<xs:annotation>
						<xs:documentation>Relationship information joining MaintLog and FlightLog records</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="elb:JoinBaseType">
								<xs:sequence>
									<xs:element name="MaintLogID" type="elb:EntityID_Type"/>
									<xs:element name="FlightLogID" type="elb:EntityID_Type"/>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="JoinMLML">
					<xs:annotation>
						<xs:documentation>Relationship information joining two MaintLog records</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="elb:JoinBaseType">
								<xs:sequence>
									<xs:element name="SourceMaintLogID" type="elb:EntityID_Type">
										<xs:annotation>
											<xs:documentation>Contains the unique ID associated with a record in MaintLog for the record forming the base of the relationship</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="TargetMaintLogID" type="elb:EntityID_Type">
										<xs:annotation>
											<xs:documentation>Contains the unique ID associated with a record in MaintLog for the record which extends from the source record in some way</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:choice minOccurs="0">
										<xs:element name="TypeOfJoin">
											<xs:annotation>
												<xs:documentation>Contains the type of relationship between two MaintLog records
										</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="20"/>
													<xs:enumeration value="RecurrenceOf">
														<xs:annotation>
															<xs:documentation>target MaintLog is a recurrence of the source MaintLog</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="RecheckFor">
														<xs:annotation>
															<xs:documentation>target MaintLog identifies a recheck requirement derived from the source MaintLog</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="FollowUpOf">
														<xs:annotation>
															<xs:documentation>target MaintLog is a follow up item related to source MaintLog</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="WorkToAddress">
														<xs:annotation>
															<xs:documentation>target MaintLog identifies work to address the source MaintLog</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="RelatedTo">
														<xs:annotation>
															<xs:documentation>target MaintLog  is related to the source MaintLog</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="SubItemOf">
														<xs:annotation>
															<xs:documentation>target MaintLog is a sub item of the source MaintLog</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="ContinuationOf">
														<xs:annotation>
															<xs:documentation>target MaintLog is a continuation of the source MaintLog</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="TypeOfJoinOther" type="elb:OtherType">
											<xs:annotation>
												<xs:documentation>Indicates a type of MaintLog relationship outside of the enumerations provided in TypeOfJoin</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:choice>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="JoinMLRS">
					<xs:annotation>
						<xs:documentation>Relationship information joining MaintLog and MaintLogResource records</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="elb:JoinBaseType">
								<xs:sequence>
									<xs:element name="MaintLogID" type="elb:EntityID_Type"/>
									<xs:element name="MaintLogResourceID" type="elb:EntityID_Type"/>
									<xs:element name="StepNumber" type="xs:int" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Contains the ordinal step number of an assigned list of maintenance activities (e.g. from MaintLog:PlannedStep)</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="JoinMRFL">
					<xs:annotation>
						<xs:documentation>Relationship information joining MaintRelease and FlightLog records</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="elb:JoinBaseType">
								<xs:sequence>
									<xs:element name="MaintReleaseID" type="elb:EntityID_Type"/>
									<xs:element name="FlightLogID" type="elb:EntityID_Type"/>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="JoinMRMA">
					<xs:annotation>
						<xs:documentation>Relationship information joining MaintRelease and MaintAction records</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="elb:JoinBaseType">
								<xs:sequence>
									<xs:element name="MaintReleaseID" type="elb:EntityID_Type"/>
									<xs:element name="MaintActionID" type="elb:EntityID_Type"/>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="JoinMRSL">
					<xs:annotation>
						<xs:documentation>Relationship information joining MaintRelease and ServiceLog records</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="elb:JoinBaseType">
								<xs:sequence>
									<xs:element name="MaintReleaseID" type="elb:EntityID_Type"/>
									<xs:element name="ServiceLogID" type="elb:EntityID_Type"/>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="JoinSLFL">
					<xs:annotation>
						<xs:documentation>Relationship information joining ServiceLog and FlightLog records</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="elb:JoinBaseType">
								<xs:sequence>
									<xs:element name="ServiceLogID" type="elb:EntityID_Type"/>
									<xs:element name="FlightLogID" type="elb:EntityID_Type"/>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="ATA_DSE_LogbookExtension" type="xs:anyType">
					<xs:annotation>
						<xs:documentation>Element that allows for non-ATA_DSE_Logbook XML schema standard elements and attributes to be added to the model</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="operator" use="required">
			<xs:annotation>
				<xs:documentation>ICAO operator code</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="5"/>
					<xs:minLength value="3"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="version" use="required">
			<xs:annotation>
				<xs:documentation>Schema version number used in each of schema's root elements</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:decimal">
					<xs:fractionDigits value="2"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ATA_S2000_Ch17_DataType">
		<xs:annotation>
			<xs:documentation>Type defining container of one or many LogbookDataContainer elements</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="LogbookDataContainer" type="elb:LogbookDataContainerType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Used to segregate logbook data contents (e.g. could be used to separate data based on aircraft or asset.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LogbookDataContainerType">
		<xs:annotation>
			<xs:documentation>Type defining container for groups of logbook data, contextual data, signatures and timestamps.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="elb:LogbookData" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Primary object data as top level element or in the LogbookDataContainer</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LogbookContextData" type="elb:ATA_DSE_LogbookType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Reference data being passed along with LogbookData for convenience and/or referential integrity</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Signature" type="ds:SignatureType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Digital Signature data structure per W3C ds definition</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Timestamp" type="xades:OtherTimeStampType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Digital timestamp data structure per W3C XAdES definition</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LogbookResponseType">
		<xs:annotation>
			<xs:documentation>Response for an ATA_DSE_Logbook object data service</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Error" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>If status=error, this element will contain a description of the error</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Code" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The receiving system code associated with the error. This code may be displayed to the user by the sending system (the system receiving the response) or recorded in a log file. Example:404</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:int">
									<xs:totalDigits value="4"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Title" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Tltle of fault that can be used by sending system to display to the user. Example: File Not Found</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="50"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Description">
							<xs:annotation>
								<xs:documentation>Detailed description of exception. Example:The file you requested cannot be found</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="500"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="MissingReferenceID" type="elb:ID_ReferenceType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Contains the unique ID of a ATA_DSE_Logbook entry. Used to list ATA_DSE_Logbook entity IDs referenced in the payload that the receiving system does not have. The system receiving this error type is expected to resend the original request including a list of all missing references.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ATA_ObjectData" type="elb:ATA_DSE_LogbookType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Contains ATA_DSE_Logbook object data</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="version" use="required">
			<xs:annotation>
				<xs:documentation>ATA_DSE_LogbookResponse XML schema version</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:decimal">
					<xs:fractionDigits value="2"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="status" use="required">
			<xs:annotation>
				<xs:documentation>The status of the web service call. The scope of the response is dependent upon the specific implementation of the system hosting the web service.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="ok"/>
					<xs:enumeration value="error"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="msgID">
			<xs:annotation>
				<xs:documentation>A reference to the message ID associated with the web service request</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="50"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>====  Complex and Simple Type Definitions  ====</xs:documentation>
	</xs:annotation>
	<xs:complexType name="AircraftDescriptorType">
		<xs:annotation>
			<xs:documentation>Contains information about the aircraft description</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AircraftManufacturerData" type="elb:AircraftManufacturerDataType">
				<xs:annotation>
					<xs:documentation>Contains data about an aircraft typically specified by the manufacturer</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AircraftOperatorData" type="elb:AircraftOperatorDataType">
				<xs:annotation>
					<xs:documentation>Contains data about an aircraft typically specified by the operator</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="AircraftIdentifierType">
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="AircraftManufacturerDataType">
		<xs:sequence>
			<xs:choice>
				<xs:element name="AircraftManufacturerName">
					<xs:annotation>
						<xs:documentation>Indicates the aircraft manufacturer</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="32"/>
							<xs:enumeration value="ATR"/>
							<xs:enumeration value="Airbus"/>
							<xs:enumeration value="Aerospatiale"/>
							<xs:enumeration value="Antonov"/>
							<xs:enumeration value="BAC"/>
							<xs:enumeration value="BAE Systems"/>
							<xs:enumeration value="Beechcraft"/>
							<xs:enumeration value="Boeing"/>
							<xs:enumeration value="Bombardier"/>
							<xs:enumeration value="Canadair"/>
							<xs:enumeration value="CASA"/>
							<xs:enumeration value="Cessna"/>
							<xs:enumeration value="Cirrus"/>
							<xs:enumeration value="Comac"/>
							<xs:enumeration value="Convair"/>
							<xs:enumeration value="Dassault"/>
							<xs:enumeration value="De Havilland"/>
							<xs:enumeration value="Douglas"/>
							<xs:enumeration value="Embraer"/>
							<xs:enumeration value="Fairchild"/>
							<xs:enumeration value="Fokker"/>
							<xs:enumeration value="Gulfstream"/>
							<xs:enumeration value="Ilyushin"/>
							<xs:enumeration value="Learjet"/>
							<xs:enumeration value="Lockheed"/>
							<xs:enumeration value="McDonnell Douglas"/>
							<xs:enumeration value="Mitsubishi"/>
							<xs:enumeration value="Piper"/>
							<xs:enumeration value="Rockwell"/>
							<xs:enumeration value="Sukhoi"/>
							<xs:enumeration value="Tupolev"/>
							<xs:enumeration value="Yakolev"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="AircraftManufacturerNameOther" type="elb:OtherType">
					<xs:annotation>
						<xs:documentation>Indicates an other aircraft manufacturer name</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="IATA_TypeCode">
				<xs:annotation>
					<xs:documentation>Describes the type of aircraft according to the IATA designation.  Example:744</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="3"/>
						<xs:minLength value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AircraftModel">
				<xs:annotation>
					<xs:documentation>Describes the type of aircraft in common use in operations for the generic designation of the model by manufacturers. Example:A330-200;777-300</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="12"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AircraftSpecification">
				<xs:annotation>
					<xs:documentation>Describes the type of aircraft related to technical specification of the A/C, internal use for an operator. Example:A330-203;777-306ER</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="12"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AircraftAMS_Model">
				<xs:annotation>
					<xs:documentation>Describes the type of aircraft related to Aircraft Maintenance Specification and MEL. Example:A330</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="12"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AircraftEngineModel">
				<xs:annotation>
					<xs:documentation>Contains the engine model installed on the aircraft. Example:Trent 800</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="elb:String12Type">
							<xs:attribute name="numberOfEngines" type="xs:int" use="required">
								<xs:annotation>
									<xs:documentation>Number of engines on the aircraft</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="AircraftFactoryDeliveryDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains the out of factory delivery date of the aircraft. Example:2012-07-02</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AircraftWarrantyEndDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains the end of the manufacturer warranty date of the aircraft. Example:2016-12-31</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AircraftOperatorDataType">
		<xs:sequence>
			<xs:element name="AircraftEffectivityCode">
				<xs:annotation>
					<xs:documentation>Contains a mandatory used reference for maintenance (e.g. manual effectivity code). Example:051</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="3"/>
						<xs:minLength value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:choice>
				<xs:element name="AircraftStatus">
					<xs:annotation>
						<xs:documentation>Contains the status of the aircraft</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="32"/>
							<xs:enumeration value="InFleet"/>
							<xs:enumeration value="OutOfFleet"/>
							<xs:enumeration value="Locked"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="AircraftStatusOther">
					<xs:annotation>
						<xs:documentation>Contains other value for aircraft status</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="32"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:choice>
			<xs:element name="AircraftOwner">
				<xs:annotation>
					<xs:documentation>Contains the name of the aircraft owner. Example:United Airlines</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="elb:String32Type">
							<xs:attribute name="typeOfOwner" type="elb:TypeOfOwnerType" use="required">
								<xs:annotation>
									<xs:documentation>Contains the type of owner. Example:Airline</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="AircraftOperator">
				<xs:annotation>
					<xs:documentation>Contains the name of the aircraft operator. Example:United Airlines</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="32"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AircraftETOPS_Capability" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains the maximum ETOPS capability (in minutes) of the aircraft within an operators operation. Note: no distinction is made between ETOPS and LROPS. Note: this is not equal to the actual ETOPS status of the aircraft. Example:90;120;138;180;207;240;330</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MaintenanceProvider" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains the name of the appointed maintenance provider / Repair Station License holder by the Operator</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="32"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AircraftInFleetDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains the date when the aircraft actually (or is projected to) entered the operator's fleet</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AircraftOutOfFleetDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains the date when the aircraft actually (or is projected to) leave the operator's fleet</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AssetType">
		<xs:sequence>
			<xs:element name="ID" type="elb:EntityID_Type">
				<xs:annotation>
					<xs:documentation>Unique ID for this Asset</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="TypeOfAsset">
					<xs:annotation>
						<xs:documentation>Typical assets tracked by separate logs</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="20"/>
							<xs:enumeration value="Aircraft"/>
							<xs:enumeration value="APU"/>
							<xs:enumeration value="Engine"/>
							<xs:enumeration value="Landing Gear"/>
							<xs:enumeration value="Nose Cowl"/>
							<xs:enumeration value="Propeller"/>
							<xs:enumeration value="Thrust Reverser"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="TypeOfAssetOther" type="elb:OtherType">
					<xs:annotation>
						<xs:documentation>Used for other value of TypeOfAsset </xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="AssetManufacturerCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>[MFR] CAGE code for asset manufacturer</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="5"/>
						<xs:maxLength value="5"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AssetSerialNum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Asset Serial Number</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="32"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="RecordName" type="elb:RecordNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A record identifier that may be easily referred to during verbal communication (i.e. not a UUID)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ParentAssetID" type="elb:EntityID_Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>To be used when TypeOfAsset is not Aircraft</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AssetPositionOnParent" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Position of this component/assembly asset on the parent asset</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AircraftDescriptor" type="elb:AircraftDescriptorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains information of the aircraft description. Can be used when TypeOfAsset=Aircraft</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Comments" type="elb:CommentsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Comments for this Asset</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AssetExtension" type="xs:anyType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Element that allows for non-ATA_DSE_Logbook XML schema standard elements and attributes to be added to the model</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="originDateTime" type="xs:dateTime" use="required">
			<xs:annotation>
				<xs:documentation>Contains the date and time when the logbook record was originally created in an electronic or paper system</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="modifiedDateTime" type="xs:dateTime">
			<xs:annotation>
				<xs:documentation>Contains the date and time when the logbook record was last modified</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="isVoid" type="xs:boolean">
			<xs:annotation>
				<xs:documentation>Indicates if the record referred to in AmendsRecordID is void. If true, Comments should contain reason for void. Note: This is not a physical deletion of the record</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:simpleType name="ATA_NumType">
		<xs:restriction base="xs:string">
			<xs:maxLength value="8"/>
			<xs:minLength value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CommentsType">
		<xs:restriction base="xs:string">
			<xs:maxLength value="1000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="CoordinateLocationType">
		<xs:sequence>
			<xs:element name="CoordinateSystem">
				<xs:annotation>
					<xs:documentation>Name of the coordinate system used</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
						<xs:enumeration value="Body"/>
						<xs:enumeration value="Wing"/>
						<xs:enumeration value="Nacelle"/>
						<xs:enumeration value="Pylon"/>
						<xs:enumeration value="HorizontalStabilizer"/>
						<xs:enumeration value="VerticalStabilizer"/>
						<xs:enumeration value="Window"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="FromStationName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Starting airframe station name identifying the location range</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="12"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ToStationName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Ending airframe station name identifying the location range</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="12"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LongitudinalAxis" type="elb:QuantityRangeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>[ASTx] This refers to Airframe X-axis coordinate range</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="HorizontalAxis" type="elb:QuantityRangeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>[BLNx] This refers to the Buttock Line value or Airframe Y-axis coordinate range. Negative values are left of longitudinal centerline, positive values are right of centerline.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VerticalAxis" type="elb:QuantityRangeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>[WATx] This refers to the Waterline Number or Airframe Z-axis coordinate range</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RadialPosition" type="elb:QuantityRangeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This refers to a radial position range for a radial coordinate system</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DocumentReferenceType">
		<xs:annotation>
			<xs:documentation>Represents reference document</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="DocumentNumber">
					<xs:annotation>
						<xs:documentation>Contains the single text string reference to a document. Example:AMM 21-51-03;SRM 52-33-05</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="100"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="DocumentNumRef">
					<xs:annotation>
						<xs:documentation>Structured document reference information</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ChapNbr" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Chapter Number of the document reference. Example:27</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:int">
										<xs:totalDigits value="2"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="SectNbr" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Section Number of the document reference. Example:51</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:int">
										<xs:totalDigits value="2"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="UnitNbr" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Unit Number of the document reference. Example:00</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:int">
										<xs:totalDigits value="2"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="TaskNbr" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Used to identify the detailed reference to the task. Example:710-052-A</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="15"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="FigureNbr" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Figure Number of the document reference (typically used for an IPC reference). Example:101</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="6"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="SheetNbr" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Sheet Number of the document reference (typically used for an IPC reference). Example:1</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="6"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="ItemNbr" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Item Number of the document reference (typically used for an IPC reference). Example:133</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="6"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="Other" minOccurs="0">
								<xs:annotation>
									<xs:documentation>To be used in not yet foreseen document references</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="15"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:element name="CAGE_Code" minOccurs="0">
				<xs:annotation>
					<xs:documentation>[CAG] Company who authored the referenced document. Example:82105</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="5"/>
						<xs:minLength value="5"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Company" minOccurs="0">
				<xs:annotation>
					<xs:documentation>[WHO] Name of the Company who authored the referenced document. Example: The Boeing Company</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="55"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Model" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Applicable aircraft model for the document. Example:A340-600</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:choice minOccurs="0">
				<xs:element name="TypeOfDocument">
					<xs:annotation>
						<xs:documentation>Description of the document title or type</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="5"/>
							<xs:enumeration value="AD">
								<xs:annotation>
									<xs:documentation>Airworthiness Directive</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="AMM">
								<xs:annotation>
									<xs:documentation>Aircraft Maintenance Manual</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="CMM">
								<xs:annotation>
									<xs:documentation>Component Maintenance Manual</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="FIM">
								<xs:annotation>
									<xs:documentation>Fault Isolation Manual</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="FRM">
								<xs:annotation>
									<xs:documentation>Fault Reporting Manual</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="MPD">
								<xs:annotation>
									<xs:documentation>Maintenance Planning Document</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="SB">
								<xs:annotation>
									<xs:documentation>Service Bulletin</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="SRM">
								<xs:annotation>
									<xs:documentation>Structural Repair Manual</xs:documentation>
